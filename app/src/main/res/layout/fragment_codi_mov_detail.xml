                android:layout_height="10dp"
                android:layout_marginStart="12dp"
                android:layout_marginEnd="12dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtInputCr"
                app:layout_constraintVertical_bias="0.0"
                app:srcCompat="@drawable/separator_line_icon" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.appcompat.widget.LinearLayoutCompat>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayoutCr"
        android:id="@+id/linearLayoutReference">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/txtReference"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:text=""
                android:textAlignment="viewStart"
                android:paddingStart="12dp"
                android:layout_marginTop="10dp"
                android:textSize="16dp"
                android:fontFamily="@font/ubuntu_bold"
                android:textColor="@color/link_color"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>
            <TextView
                android:id="@+id/txtInputReference"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintStart_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtReference"
                android:fontFamily="@font/ubuntu_bold"
                android:textColor="@color/black"
                android:layout_marginTop="8dp"
                android:textSize="16dp"
                android:paddingStart="12dp"
                android:text="251sassadgsdgs2gd"
