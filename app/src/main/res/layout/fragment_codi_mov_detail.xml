                android:id="@+id/txtAccount"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:text="NÃºmero de cuenta"
                android:textAlignment="viewStart"
                android:paddingStart="12dp"
                android:layout_marginTop="8dp"
                android:textSize="16dp"
                android:fontFamily="@font/ubuntu_bold"
                android:textColor="@color/link_color"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>
            <TextView
                android:id="@+id/txtInputAccount"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintStart_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtAccount"
                android:fontFamily="@font/ubuntu_bold"
                android:textColor="@color/black"
                android:layout_marginTop="4dp"
                android:textSize="16dp"
                android:paddingStart="12dp"
                android:text="251sassadgsdgs2gd"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="10dp"
                android:layout_marginStart="12dp"
                android:layout_marginEnd="12dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtInputAccount"
                app:layout_constraintVertical_bias="0.0"
                app:srcCompat="@drawable/separator_line_icon" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.appcompat.widget.LinearLayoutCompat>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="65dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayoutAccount"
        android:id="@+id/linearLayoutConcept">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/txtConcept"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:text="Concepto"
                android:textAlignment="viewStart"
                android:paddingStart="12dp"
                android:layout_marginTop="8dp"
                android:textSize="16dp"
                android:fontFamily="@font/ubuntu_bold"
                android:textColor="@color/link_color"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>
            <TextView
                android:id="@+id/txtInputConcept"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintStart_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtConcept"
                android:fontFamily="@font/ubuntu_bold"
                android:textColor="@color/black"
                android:layout_marginTop="4dp"
                android:textSize="16dp"
                android:paddingStart="12dp"
                android:text="251sassadgsdgs2gd"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="10dp"
                android:layout_marginStart="12dp"
                android:layout_marginEnd="12dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtInputConcept"
                app:layout_constraintVertical_bias="0.0"
                app:srcCompat="@drawable/separator_line_icon" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.appcompat.widget.LinearLayoutCompat>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="65dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayoutConcept"
        android:id="@+id/linearLayoutDate">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/txtDate"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:text="Fecha y Hora de solicitud"
                android:textAlignment="viewStart"
                android:paddingStart="12dp"
                android:layout_marginTop="8dp"
                android:textSize="16dp"
                android:fontFamily="@font/ubuntu_bold"
                android:textColor="@color/link_color"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>
            <TextView
                android:id="@+id/txtInputDate"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintStart_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtDate"
                android:fontFamily="@font/ubuntu_bold"
                android:textColor="@color/black"
                android:layout_marginTop="4dp"
                android:textSize="16dp"
                android:paddingStart="12dp"
                android:text="251sassadgsdgs2gd"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="10dp"
                android:layout_marginStart="12dp"
                android:layout_marginEnd="12dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtInputDate"
                app:layout_constraintVertical_bias="0.0"
                app:srcCompat="@drawable/separator_line_icon" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.appcompat.widget.LinearLayoutCompat>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="65dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayoutDate"
        android:id="@+id/linearLayoutPhone">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
