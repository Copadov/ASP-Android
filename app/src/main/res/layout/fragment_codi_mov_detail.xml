<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/detailMovSales"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.fragments.codi.CodiMovDetailFragment"
    android:layout_marginTop="15dp"
    android:background="@color/colorPrimaryLightMaterial70" android:nestedScrollingEnabled="true">

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="65dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/linearLayoutFolio">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/txtFolio"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:text="Folio"
                android:textAlignment="viewStart"
                android:paddingStart="12dp"
                android:layout_marginTop="8dp"
                android:textSize="16dp"
                android:fontFamily="@font/ubuntu_bold"
                android:textColor="@color/link_color"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>
            <TextView
                android:id="@+id/txtInputFolio"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintStart_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtFolio"
                android:fontFamily="@font/ubuntu_bold"
                android:textColor="@color/black"
                android:layout_marginTop="4dp"
                android:textSize="16dp"
                android:paddingStart="12dp"
                android:text="251sassadgsdgs2gd"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="10dp"
                android:layout_marginStart="12dp"
                android:layout_marginEnd="12dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtInputFolio"
                app:layout_constraintVertical_bias="0.0"
                app:srcCompat="@drawable/separator_line_icon" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.appcompat.widget.LinearLayoutCompat>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="65dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/linearLayoutFolio"
        android:id="@+id/linearLayoutAccount">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
